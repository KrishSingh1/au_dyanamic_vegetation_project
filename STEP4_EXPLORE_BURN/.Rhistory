corner <- site.coords[c(SW,SE,NE, NW),] # get corner rows
corner <- corner[,c('V1', 'X', 'Y', 'Z')]
colnames(corner)[1] <- 'Name'
corner.points <- rbind(corner.points, corner) # bind to template df
}
